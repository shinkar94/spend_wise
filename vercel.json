{
  "version": 2,
  "builds": [
    {
      "src": "src/app/page/**",
      "use": "@vercel/node"
    },
    {
      "src": "src/api/myMoney/route.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/myMoney/(.*)",
      "dest": "src/api/myMoney/route.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/app/page/$1"
    }
  ]
}